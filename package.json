{
  "name": "rbjks.github.io",
  "private": true,
  "description": "Access this website at https://rbjks.github.io/index.html",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "fix": "concurrently --kill-others-on-fail --prefix-colors \"auto\" \"npm:fix:*\"",
